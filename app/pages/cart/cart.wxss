.car {
  padding-top: 280rpx;
  text-align: center;
}

.car>image {
  width: 220rpx;
  height: 220rpx;
}

.car>view {
  font-size: 24rpx;
  margin-top: 18rpx;
}

.car>view:nth-child(1) {
  color: #999;
}

.car>view:nth-child(2) {
  color: #333;
}

.my-scroll {
  width: 100%;
  white-space: nowrap;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10000;
  background: #fff;
}

.s-item {
  padding: 0 20rpx;
  display: inline-block;
  font-size: 26rpx;
  height: 80rpx;
  line-height: 80rpx;
}

.s-item>image {
  width: 20rpx;
  height: 11rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}

#img1 {
  padding-left: 30rpx;
}

.heji {
  height: 60rpx;
  line-height: 60rpx;
  background: #d9e1f0;
  font-size: 24rpx;
  color: #333;
  margin-top: 80rpx;
}

.cart_container {
  display: flex;
  flex-direction: row;
}

.scroll {
  margin-bottom: 120rpx;
}

.column {
  display: flex;
  flex-direction: column;
}

.row {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.sku {
  margin-top: 60rpx;
  margin-left: 100rpx;
}

.sku-price {
  color: red;
  position: relative;
  margin-top: 70rpx;
}

.price {
  color: red;
  position: relative;
}

.title {
  font-size: 38rpx;
  margin-top: 40rpx;
}

.small_text {
  font-size: 28rpx;
  margin-right: 40rpx;
  margin-left: 10rpx;
}

.item-select {
  width: 40rpx;
  height: 40rpx;
  margin-top: 90rpx;
  margin-left: 20rpx;
}

.item-allselect {
  width: 40rpx;
  height: 40rpx;
  margin-left: 20rpx;
}

.item-image {
  width: 180rpx;
  height: 180rpx;
  margin: 20rpx;
}

.bottom_line {
  width: 100%;
  height: 2rpx;
  background: lightgray;
}

.bottom_total {
  position: fixed;
  display: flex;
  flex-direction: column;
  bottom: 0;
  width: 100%;
  height: 120rpx;
  line-height: 120rpx;
  background: white;
}

.button-red {
  background-color: #f44336; /* 红色 */
}

button {
  position: fixed;
  right: 0;
  color: white;
  text-align: center;
  display: inline-block;
  font-size: 30rpx;
  border-radius: 0rpx;
  width: 30%;
  height: 120rpx;
  line-height: 120rpx;
}

.cart-box {
  background: #fff;
}

.cart-box>view {
  border-bottom: 1rpx solid #e8e8e8;
}

.pro-car {
  display: flex;
  padding: 20rpx 0 10rpx 0;
}

.icon {
  width: 120rpx;
  display: flex;
  align-items: center;
}

navigator {
  margin-right: 20rpx;
}

navigator>image {
  width: 170rpx;
  height: 170rpx;
}

.car-msg {
  font-size: 24rpx;
  color: #333;
}

.car-msg>view:nth-child(2) {
  margin-top: 10rpx;
  color: #999;
  margin-bottom: 15rpx;
}

.qian {
  color: #fd9900;
  font-size: 30rpx;
  font-weight: bold;
  margin-top: 20rpx;
  display: table-cell;
  vertical-align: bottom;
}

.add {
  margin-top: 20rpx;
  margin-left: 10rpx;
}

.car-jia {
  display: flex;
  justify-content: space-between;
}

.shou {
  color: #999;
  font-weight: normal;
  margin-left: 20rpx;
  font-size: 24rpx;
  margin-top: 5rpx;
}

.total {
  text-align: right;
  font-size: 22rpx;
  padding: 20rpx 30rpx;
  background: #fff;
  margin-bottom: 20rpx;
}

.total>text:nth-child(2) {
  font-size: 30rpx;
  color: #333;
}

.all-car {
  background: #f4f5f6;
}

.xian {
  color: #fd9900;
}

.shiX {
  display: flex;
  justify-content: space-between;
  background: #fff;
  height: 80rpx;
  line-height: 80rpx;
  color: #333;
  font-size: 26rpx;
  font-weight: bold;
  border-bottom: 1rpx solid #e8e8e8;
}

.shiX>view:nth-child(2) {
  color: #0097ff;
}

.SX>view {
  font-size: 22rpx;
  height: 30rpx;
  width: 60rpx;
  background: #ccc;
  text-align: center;
  line-height: 30rpx;
  border-radius: 15rpx;
  color: #fff;
  margin-right: 15rpx;
}

.shi>.plus {
  background: #ccc;
  color: #fff;
  border-color: #ccc;
}

.shi>.redu {
  display: none;
}

.shi>.input {
  display: none;
}

.btm {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 100rpx;
  width: 100%;
  background: #fafafa;
  display: flex;
  justify-content: space-between;
}

.jieS {
  display: flex;
}

.jieS>view:first-child {
  text-align: right;
}

.jieS text {
  color: red;
  font-size: 22rpx;
}

.zong {
  font-size: 20rpx;
  color: #333;
}

.zong>text {
  color: #fd9900;
  font-size: 30rpx;
}

.jieS>view:last-of-type {
  width: 180rpx;
  background: #0097ff;
  text-align: center;
  line-height: 100rpx;
  color: #fff;
  font-size: 26rpx;
  margin-left: 20rpx;
}

.btm>.icon {
  padding-left: 30rpx;
  font-size: 26rpx;
}

.touch-item {
  font-size: 14px;
  display: flex;
  justify-content: space-between;
  width: 100%;
  overflow: hidden;
}

.content {
  width: 100%;
  margin-right: 0;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  margin-left: -90px;
}

.del {
  background-color: orangered;
  width: 90px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(90px);
  transform: translateX(90px);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  margin-bottom: 20rpx;
}

.touch-move-active .content, .touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.con {
  padding-bottom: 120rpx;
}

.active {
  border-color: #0097ff;
  color: #0097ff;
}
